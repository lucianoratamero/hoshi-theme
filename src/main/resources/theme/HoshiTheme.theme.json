{
  "name": "HoshiTheme",
  "dark": true,
  "author": "Luciano Ratamero",
  "editorScheme": "theme/Hoshi.xml",
  "colors": {
    "black": "#131617",
    "blackS2": "#13161788",
    "blackD1": "#07090a",
    "white": "#eeeeee",
